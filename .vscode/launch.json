    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Web API",
            "protocol": "inspector",
            "program": "${workspaceFolder}/web-api/dist/restful-server.js",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Test",
            "protocol": "inspector",
            "program": "${workspaceFolder}/mobile-client/.bin/copyShared.js",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Web Cliet",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceRoot}/web-admin",
            "sourceMaps": true,
            "port": 9223,
            "userDataDir": "${workspaceRoot}/.vscode/chrome"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Mobile Client",
            "url": "http://localhost:8100",
            "webRoot": "${workspaceRoot}/web-client/www"
        }
    ]
}